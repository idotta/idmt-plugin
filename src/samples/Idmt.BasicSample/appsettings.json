{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Idmt": {
    "MultiTenant": {
      "DefaultTenantId": "system-tenant",
      "Strategies": ["header", "claim"],
      "StrategyOptions": {
        "header": "__tenant-identifier__",
        "claim": "tenant-identifier"
      }
    }
  }
}